{
  "plugins": [
    "babel-plugin-twin",
    [
      "babel-plugin-styled-components",
      {
        "namespace": "sc-",
        "pure": true,
        "ssr": false
      }
    ],
    [
      "babel-plugin-macros",
      {
        "twin": {
          "preset": "styled-components",
          "styled": {
            "import": "default",
            "from": "styled-components/macro"
          },
          "css": {
            "import": "css",
            "from": "styled-components/macro"
          }
        },
        "styledComponents": {
          "namespace": "sc-",
          "pure": true,
          "ssr": false
        }
      }
    ]
  ]
}
